<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Elvl19">
    <nl.dionsegijn.konfetti.KonfettiView
        android:id="@+id/conffetti"
        android:layout_height="match_parent"
        android:layout_width="match_parent"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="370dp"
        android:layout_height="69dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:fontFamily="@font/bree_serif"
        android:text="@string/lvl1"
        android:textAlignment="center"
        android:textColor="#673AB7"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/GraphMain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.775" />

    <ImageView
        android:id="@+id/GraphMain"
        android:layout_width="276dp"
        android:layout_height="278dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.403"
        app:srcCompat="@drawable/elvl19" />

    <ImageView
        android:id="@+id/v1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:onClick="v1Tap"
        app:layout_constraintBottom_toBottomOf="@+id/GraphMain"
        app:layout_constraintEnd_toEndOf="@+id/GraphMain"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/GraphMain"
        app:layout_constraintTop_toTopOf="@+id/GraphMain"
        app:layout_constraintVertical_bias="0.02"
        app:srcCompat="@drawable/gray"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/v2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:onClick="v2Tap"
        app:layout_constraintBottom_toBottomOf="@+id/GraphMain"
        app:layout_constraintEnd_toEndOf="@+id/GraphMain"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/GraphMain"
        app:layout_constraintTop_toTopOf="@+id/GraphMain"
        app:layout_constraintVertical_bias="0.56"
        app:srcCompat="@drawable/gray"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/v4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:onClick="v4Tap"
        app:layout_constraintBottom_toBottomOf="@+id/GraphMain"
        app:layout_constraintEnd_toEndOf="@+id/GraphMain"
        app:layout_constraintHorizontal_bias="0.977"
        app:layout_constraintStart_toStartOf="@+id/GraphMain"
        app:layout_constraintTop_toTopOf="@+id/GraphMain"
        app:layout_constraintVertical_bias="0.977"
        app:srcCompat="@drawable/gray"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/v3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:onClick="v3Tap"
        app:layout_constraintBottom_toBottomOf="@+id/GraphMain"
        app:layout_constraintEnd_toEndOf="@+id/GraphMain"
        app:layout_constraintHorizontal_bias="0.024"
        app:layout_constraintStart_toStartOf="@+id/GraphMain"
        app:layout_constraintTop_toTopOf="@+id/GraphMain"
        app:layout_constraintVertical_bias="0.977"
        app:srcCompat="@drawable/gray"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/l1check"
        android:layout_width="125dp"
        android:layout_height="35dp"
        android:background="@drawable/echeck"
        android:fontFamily="@font/baloo"
        android:onClick="check"
        android:text="Check"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow"
        app:layout_constraintVertical_bias="0.418"
        tools:ignore="TouchTargetSizeCheck" />

    <TableRow
        android:id="@+id/tableRow"
        android:layout_width="283dp"
        android:layout_height="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/GraphMain"
        app:layout_constraintVertical_bias="0.224">

        <Space
            android:layout_width="@dimen/Space"
            android:layout_height="32dp" />

        <ImageView
            android:id="@+id/sc1"
            android:layout_width="@dimen/colorBtn"
            android:layout_height="@dimen/colorBtn"
            android:onClick="red"
            app:srcCompat="@drawable/red"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />


        <Space
            android:layout_width="@dimen/Space2"
            android:layout_height="32dp" />

        <ImageView
            android:id="@+id/sc2"
            android:layout_width="@dimen/colorBtn"
            android:layout_height="@dimen/colorBtn"
            android:onClick="yellow"
            app:srcCompat="@drawable/yellow"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <Space
            android:layout_width="@dimen/Space2"
            android:layout_height="32dp" />

        <ImageView
            android:id="@+id/sc3"
            android:layout_width="@dimen/colorBtn"
            android:layout_height="@dimen/colorBtn"
            android:onClick="pink"
            app:srcCompat="@drawable/pink"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck,TouchTargetSizeCheck" />

        <Space
            android:layout_width="@dimen/Space2"
            android:layout_height="32dp" />

        <ImageView
            android:id="@+id/sc4"
            android:layout_width="@dimen/colorBtn"
            android:layout_height="@dimen/colorBtn"
            android:onClick="green"
            app:srcCompat="@drawable/green"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <Space
            android:layout_width="@dimen/Space2"
            android:layout_height="32dp" />

        <ImageView
            android:id="@+id/sc5"
            android:layout_width="@dimen/colorBtn"
            android:layout_height="@dimen/colorBtn"
            android:onClick="cyan"
            app:srcCompat="@drawable/cyan"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <Space
            android:layout_width="@dimen/Space"
            android:layout_height="32dp" />

    </TableRow>

    <Button
        android:id="@+id/l1check2"
        android:layout_width="125dp"
        android:layout_height="35dp"
        android:background="@drawable/purple"
        android:fontFamily="@font/baloo"
        android:onClick="clrAll"
        android:text="Clear All"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toEndOf="@+id/l1check"
        app:layout_constraintTop_toBottomOf="@+id/tableRow"
        app:layout_constraintVertical_bias="0.418"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bree_serif"
        android:shadowColor="#070000"
        android:text="LEVEL 19"
        android:textColor="@color/eLvlH"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FF0000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.661" />

</androidx.constraintlayout.widget.ConstraintLayout>